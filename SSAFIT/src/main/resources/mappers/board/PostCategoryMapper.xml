<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafit.domain.board.dao.PostCategoryDao">

    <select id="selectAll" resultType="PostCategory">
        SELECT
            id,
            category
        FROM post_category
        ORDER BY id ASC
    </select>

</mapper>
